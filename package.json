    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "qs": "^6.11.2",
    "react": "17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-bootstrap": "^2.8.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.17.0",
    "react-portal": "^4.2.1",
    "react-router-dom": "^5.3.3",
    "reconnecting-websocket": "^4.4.0",
    "semantic-ui-react": "^2.1.2",
