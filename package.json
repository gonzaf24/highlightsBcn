{
  "name": "highlightsbcn-dashboard-app",
  "description": "highlightsbcn-dashboard-app",
  "version": "0.0.1",
  "private": true,
  "homepage": ".",
  "license": "PRIVATE",
  "scripts": {
    "start": "dotenv -e ./envs/local.env -- react-app-rewired start --config-overrides ./webpack/webpack.config.dev.js",
    "build": "dotenv -e ./envs/production.env react-app-rewired build --config-overrides ./webpack/webpack.config.prod.js",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "serve": "serve -s -d -l 443 --ssl-cert ./certs/cert.pem --ssl-key ./certs/key.pem build",
    "dev": "npm run development",
    "development": "dotenv -e ./envs/development.env -- react-app-rewired build --config-overrides ./webpack/webpack.config.dev.js",
    "prod": "npm run production",
    "production": "dotenv -e ./envs/production.env -- react-app-rewired build --config-overrides ./webpack/webpack.config.prod.js",
    "stage": "npm run staging",
    "staging": "dotenv -e ./envs/staging.env -- react-app-rewired build --config-overrides ./webpack/webpack.config.prod.js",
    "eslint": "eslint ./ --ext .js --ext .jsx",
    "eslint-fix": "eslint ./ --ext .js --ext .jsx --fix",
    "clean": "rm -rf node_modules && rm -f package-lock.json",
    "reinstall": "npm run clean && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.8.2",
    "@testing-library/jest-dom": "^5.16.4",
