    "babel-loader": "^8.1.0",
    "clipboardy": "^3.0.0",
    "css-loader": "^6.6.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.17.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
